<div class="container">
    <h1>Carousel Page</h1>
</div>
<div class="container">
    <!-- if carousel -->
    {{#if carousel}} 
        {{#each carousel}}
            <div>
                <h4 class="text-muted">{{title}} - {{author.name.first}} {{author.name.last}}</h4>
                <p>{{content}}</p>
            </div>
        {{/each}} 
    {{else}}
    <!-- else -->
    {{/if}}
</div>